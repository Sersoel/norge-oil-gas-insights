{
  "name": "norge-oil-gas-insights",
  "version": "0.7.0",
  "private": true,
  "type": "module",
  "scripts": {
    "fetch:data": "python scripts/fetch_convert.py", // python scripts/convert_iho_to_geojson.py
    "sync:data": "cp data/processed/*.csv public/assets/data/ && cp data/processed/*.json public/assets/data/ && cp data/processed/*.geojson public/assets/data/",
    "dev": "vite",
    "dev:full": "npm run fetch:data && npm run sync:data && vite",
    "build": "vite build",
    "build:all": "npm run fetch:data && npm run sync:data && vite build",
    "preview": "vite preview",
    "lint": "eslint . --fix",
    "format": "prettier --write src/"
  },
  "dependencies": {
    "d3": "^7.9.0",
    "vue": "^3.5.13"
  },
  "devDependencies": {
    "@eslint/js": "^9.22.0",
    "@vitejs/plugin-vue": "^5.2.3",
    "@vue/eslint-config-prettier": "^10.2.0",
    "eslint": "^9.22.0",
    "eslint-plugin-vue": "~10.0.0",
    "globals": "^16.0.0",
    "prettier": "3.5.3",
    "vite": "^6.2.4",
    "vite-plugin-vue-devtools": "^7.7.2"
  }
}
